# RobovAI Nova (PROJECT_NOVA) environment template
# Copy to .env and fill with real values (do NOT commit secrets)

# Runtime
ENVIRONMENT=development
PORT=8000
EXTERNAL_URL=http://127.0.0.1:8000
FRONTEND_URL=http://127.0.0.1:8000
DATABASE_PATH=users.db
# Generate locally and replace (example): python -c "import secrets; print(secrets.token_urlsafe(64))"
JWT_SECRET_KEY=replace_with_min_32_char_secret

# Required for centralized external verification API
EXTERNAL_API_KEY=replace_with_shared_external_api_key

# Telegram bot (optional locally, required in production bot mode)
TELEGRAM_BOT_TOKEN=

# AI keys (set what you use)
GROQ_API_KEY=
GROQ_API_KEY_2=
GROQ_API_KEY_3=
GROQ_API_KEY_4=
NVIDIA_API_KEY=
OPENROUTER_API_KEY=

# Optional image providers
UNSPLASH_ACCESS_KEY=
PEXELS_API_KEY=
IMGBB_API_KEY=
